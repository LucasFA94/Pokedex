<%- include("partials/header") %>
        <h1>Hello Mundo</h1>
        <div class="container bg-secondary text-center">
            <img src=<%= pokemon.pictures.oficial %>>
            <div class="row bg-success">
                <div class="col">
                    <h2>Name</h2>
                </div>
                <div class="col">
                    <h2>Language: Name</h2>
                </div>
                <div class="col">
                    <h2>No.</h2>
                </div>
                <div class="col">
                    <h2>Gender Ratio</h2>
                </div>
                <div class="col">
                    <h2>Type</h2>
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <h2><%= pokemon.name %></h2>
                </div>
                <div class="col">
                    <ul style="list-style-type: none;">
                        <% for(var i=0; i<pokemon.nameList.length; i++) {%>
                           <li> <i class="flag flag-united-states"></i> <%= pokemon.nameList[i].language %>: <%= pokemon.nameList[i].name %></li>
                        <% } %>
                    </ul>
                </div>
                <div class="col">
                    <h2><%= pokemon.no %></h2>
                </div>
                <div class="col">
                    <h2>Working</h2>
                </div>
                <div class="col">
                    <h2 class="type"><%= pokemon.types %></h2>
                </div>
            </div>
            <div class="row bg-success">
                <div class="col">
                    <h2>Classification</h2>
                </div>
                <div class="col">
                    <h2>Height</h2>
                </div>
                <div class="col">
                    <h2>Weight</h2>
                </div>
                <div class="col">
                    <h2>Capture Rate</h2>
                </div>
                <div class="col">
                    <h2>Base Egg Steps</h2>
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <h2><%= pokemon.classification %></h2>
                </div>
                <div class="col">
                    <h2><%= pokemon.height %></h2>
                </div>
                <div class="col">
                    <h2><%= pokemon.weight %></h2>
                </div>
                <div class="col">
                    <h2>Working</h2>
                </div>
                <div class="col">
                    <h2>Working</h2>
                </div>
            </div>
        </div>
<%- include("partials/footer") %>